{
    "manifest_version": 3,
    "name": "Always On Top Window Demo",
    "version": "1.0",
    "description": "Demo for always on top window",
    "permissions": [
      "notifications",
      "tabs",
      "system.display"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_title": "Open Always On Top Window"
    }
  }